{
  "name": "cryptocurrencies-monitor",
  "version": "1.0.0",
  "description": "Rest API built with NodeJS that is as a wrapper for the CoinGecko API",
  "main": "app.js",
  "scripts": {
    "dev": "nodemon app.js",
    "test": "mocha",
    "create-db": "npx sequelize-cli db:create",
    "migrations": "npx npx sequelize-cli db:migrate",
    "start": "npm install && npm run create-db && npm run migrations && npm run dev"
  },
  "author": "Natalia Maccarrone",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mysql2": "^2.2.5",
    "sequelize": "^6.4.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "mocha": "^8.2.1",
    "nodemon": "^2.0.7",
    "prettier": "2.2.1",
    "sinon": "^9.2.4"
  }
}
